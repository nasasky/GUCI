page {
  background-color: #ffffff;
}
.recharge-hrader {
  background: #fff;
  width: 100%;
  height: 200rpx;
  align-items: center;
  background-image: url(data:image/png;base64,);
  background-size: 100%;
  position: relative;
}
.recharge-hrader .name {
  display: flex;
  padding-left: 30rpx;
  height: 70rpx;
  align-items: center;
  font-size: 26rpx;
  color: #27323f;
  padding-top: 10rpx;
}
.recharge-hrader .money {
  font-size: 80rpx;
  padding-left: 30rpx;
  display: flex;
  height: 100rpx;
  align-items: center;
  color: #27323f;
  padding-top: 12rpx;
}
.recharge-hrader .money text {
  font-size: 40rpx;
  padding-top: 20rpx;
  padding-right: 10rpx;
}
.recharge-hrader .info {
  width: 130rpx;
  height: 42rpx;
  border-radius: 4rpx;
  color: rgba(39, 50, 63, 0.6);
  border: 2rpx solid rgba(39, 50, 63, 0.2);
  text-align: center;
  font-size: 24rpx;
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.recharge-money-box .input-money {
  border: 2rpx solid #f5f5f5;
  width: 626rpx;
  height: 60rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  padding: 10rpx;
  color: #27323f;
  border-radius: 6rpx;
  padding-left: 20rpx;
  font-size: 30rpx;
}
.recharge-money-box .recharge-money-list {
  width: 660rpx;
  border: 2rpx solid #f5f5f5;
  margin: 0 auto;
  margin-top: 30rpx;
  border-radius: 6rpx;
}
.recharge-money-box .recharge-money-list .list-name {
  color: #27323f;
  display: flex;
  height: 72rpx;
  align-items: center;
  padding-left: 20rpx;
  border-bottom: 2rpx solid #f5f5f5;
}
.recharge-money-box .recharge-money-list .list-name text {
  color: #7b8196;
  font-size: 24rpx;
  padding-left: 12rpx;
}
.recharge-money-box .recharge-money-list .list-box {
  display: flex;
  flex-wrap: wrap;
  padding-top: 20rpx;
  padding-bottom: 40rpx;
}
.recharge-money-box .recharge-money-list .list-box .list {
  width: 46%;
  height: 200rpx;
  background-color: #a18d65;
  margin-top: 18rpx;
  margin-left: 18rpx;
  box-shadow: 0 0 16rpx #eee;
  border-radius: 6rpx;
}
.recharge-money-box .recharge-money-list .list-box .list .name {
  font-size: 60rpx;
  padding-left: 22rpx;
  display: flex;
  height: 120rpx;
  align-items: center;
  color: #ffffff;
}
.recharge-money-box .recharge-money-list .list-box .list .name text {
  font-size: 40rpx;
  padding-top: 12rpx;
  padding-right: 8rpx;
}
.recharge-money-box .recharge-money-list .list-box .list .info,
.recharge-money-box .recharge-money-list .list-box .list .money {
  font-size: 24rpx;
  color: #ffffff;
  padding-left: 24rpx;
}
.recharge-money-box .recharge-button {
  position: fixed;
  width: 100%;
  height: 90rpx;
  bottom: 0;
  background-color: #ffd305;
  color: #27323f;
  line-height: 80rpx;
  font-size: 30rpx;
  box-shadow: 0 0 12rpx #eee;
  border-radius: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.recharge-money-box .recharge-button::after {
  border: 0;
}
