page {
  background-color: #ffffff;
}
.sales-title {
  width: 90%;
  margin: 0 auto;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.goods-list {
  border-bottom: 2rpx solid #f5f5f5;
}
.goods-list .selectTap {
  width: 90rpx;
  height: 120rpx;
  transition: margin-left 0.2s ease-in-out;
  background: url(data:image/png;base64,) no-repeat 30rpx center;
  background-size: 34rpx auto;
}
.goods-list .active {
  background: url(data:image/png;base64,) no-repeat 30rpx center;
  background-size: 34rpx auto;
}
.goods-list .images {
  width: 130rpx;
  height: 130rpx;
}
.goods-list .name {
  color: #27323f;
}
.goods-list .spec {
  color: #7b8196;
  font-size: 26rpx;
  padding-top: 6rpx;
}
.goods-list .number-box {
  display: flex;
  width: 100%;
  height: 50rpx;
  align-items: center;
}
.goods-list .number-box .price {
  color: #a18d65;
  width: 60%;
}
.goods-footer {
  position: absolute;
  width: 100%;
  bottom: 0;
  box-shadow: 0 0px 15px 0 #eaeaea;
  background-color: #fff;
}
.goods-footer .sele-box {
  display: flex;
  align-items: center;
}
.goods-footer .sele-box .selectTap {
  width: 70rpx;
  height: 100rpx;
  transition: margin-left 0.2s ease-in-out;
  background: url(data:image/png;base64,) no-repeat 30rpx center;
  background-size: 34rpx auto;
}
.goods-footer .sele-box .active {
  background: url(data:image/png;base64,) no-repeat 30rpx center;
  background-size: 34rpx auto;
}
.goods-footer .sele-box .text {
  color: #27323f;
  padding-left: 10rpx;
}
.goods-footer .button {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #ffd305;
  color: #27323f;
}
.goods-footer .nonebutton {
  color: #fff;
  background-color: #7b8196;
}
.no-more-goods {
  width: 100%;
  height: 500rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.no-more-goods .no-order-img {
  width: 200rpx;
  height: 200rpx;
}
.no-more-goods .text {
  font-size: 26rpx;
  color: #bfbfbf;
}
