page {
  background-color: #fff;
}
.wuliu-header {
  background-color: #fff;
}
.wuliu-header image {
  width: 90rpx;
  height: 90rpx;
}
.wuliu-header .name {
  color: #7b8196;
  padding-top: 6rpx;
  padding-bottom: 6rpx;
}
.wuliu-header .name text {
  color: #27323f;
}
.wuliu-header .button {
  border: 2rpx solid #e2e2e2;
  color: #7b8196;
  font-size: 24rpx;
  padding: 2rpx 10rpx;
  border-radius: 4rpx;
}
.wuliu-list {
  border-top: 20rpx solid #f5f5f5;
}
.wuliu-list .line {
  border-left: 2rpx dashed rgba(123, 129, 150, 0.3);
  height: 160rpx;
  position: relative;
}
.wuliu-list .line .after {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 26rpx;
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  bottom: 42%;
  right: -12rpx;
}
.wuliu-list .time {
  color: #7b8196;
  font-size: 24rpx;
  padding-bottom: 2rpx;
}
.wuliu-list .text {
  color: #27323f;
  font-size: 26rpx;
  width: 95%;
}
.no-more-goods {
  width: 100%;
  height: 500rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.no-more-goods .no-order-img {
  width: 200rpx;
  height: 200rpx;
}
.no-more-goods .text {
  font-size: 26rpx;
  color: #bfbfbf;
  width: 400rpx;
  text-align: center;
}
